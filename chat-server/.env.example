# 服务器配置
PORT=3001
NODE_ENV=production

# GitHub OAuth 配置
# 创建OAuth App: https://github.com/settings/developers
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_CALLBACK_URL=http://localhost:3001/auth/github/callback

# Session 密钥（请更换为随机字符串）
SESSION_SECRET=your_random_session_secret_here_change_this

# CORS 允许的前端地址（多个用逗号分隔）
ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080,https://study.wenqi.icu

# 前端地址（OAuth成功后重定向）
FRONTEND_URL=http://localhost:8080
